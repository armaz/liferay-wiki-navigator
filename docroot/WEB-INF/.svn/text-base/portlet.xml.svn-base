<?xml version="1.0"?>

<portlet-app
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
>
	
	<portlet>
		<portlet-name>wikinavigator</portlet-name>
		<display-name>WikiNavigator</display-name>
		<portlet-class>
			no.uninett.fas.agora.WikiNavigator
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/wikinavigator/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/wikinavigator/edit.jsp</value>
		</init-param>
		<init-param>
			<name>help-jsp</name>
			<value>/html/wikinavigator/help.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>help</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>WikiNavigator</title>
			<short-title>WikiNavigator</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
        <supported-public-render-parameter>title</supported-public-render-parameter>
	</portlet>
    <public-render-parameter>
        <identifier>title</identifier>
        <qname xmlns:x="http://www.liferay.com/public-render-parameters/wiki">x:title</qname>
    </public-render-parameter>
</portlet-app>